{
  "manifest_version": 2,

  "name": "Thanos Demands Silence",
  "description": "Trying to save you from Avengers Endgame Spoiler.",
  "version": "1.0",
  "icons": { 
          "96": "black-avengers-png-logo-2.png" },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["jquery-3.4.0.min.js", "hide-spoiler.js"],
      "run_at": "document_end"
    }
  ]
}
